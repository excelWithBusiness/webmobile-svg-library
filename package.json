{
  "name": "@excelwithbusiness/webmobile-lib-svg",
  "version": "0.0.1",
  "description": "A lib that holds all the SVGs and corresponding react components.",
  "license": "UNLICENSED",
  "sideEffects": false,
  "main": "dist/index",
  "module": "dist/index",
  "scripts": {
    "build": "yarn clean && cross-env BABEL_ENV=build NODE_ENV=production run-p build:babel",
    "build:babel": "babel src --out-dir dist --config-file ./babel.config.js --source-maps --extensions .ts,.tsx,.js --no-comments",
    "build:svg": "rimraf -rf ./src/components && cross-env UV_THREADPOOL_SIZE=64 babel-node --extensions .ts,.tsx,.js --config-file ./babel.config.js ./build.ts && yarn build ",
    "clean": "rimraf -rf ./dist/*",
    "generate:dts": "tsc --project tsconfig.build.json",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.7.4",
    "@babel/parser": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@svgr/cli": "^5.4.0",
    "@svgr/core": "^5.4.0",
    "@types/async": "^3.2.3",
    "@types/enzyme": "^3.10.8",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.19",
    "@types/mustache": "4.0.1",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.9.53",
    "@types/rimraf": "^3.0.0",
    "@types/styled-components": "^5.1.5",
    "@types/svgo": "^1.3.3",
    "@types/temp": "^0.8.34",
    "async": "^3.2.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.11.0",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mustache": "^4.0.1",
    "mustache-jest": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "svgo": "^1.3.2",
    "temp": "^0.9.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "dependencies": {
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
